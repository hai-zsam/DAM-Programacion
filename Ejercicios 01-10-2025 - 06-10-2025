# Ejercicio 7, PDF 2

Algoritmo Ejercicio7Parte2
	
	Escribir "Introduce tu calificación como número entero."
	Leer calificacion
	
	Si calificacion <= 2 Entonces
		Escribir "Su nota es muy Deficiente."
	SiNo
		
		Si calificacion >= 3  y calificacion < 5 Entonces
			Escribir "Su calificacion es Insuficiente."
		Sino
			Si calificacion = 5 o calificacion = 6 Entonces
				Escribir "Su calificacion es Bien."
			Sino 
				Si calificacion >= 7 y calificacion < 9 Entonces
					Escribir "Su calificacion es Notable."
				SiNo
					Si calificacion = 9 o calificacion = 10 Entonces
						Escribir "Su calificacion es Sobresaliente."
					FinSi
			
				FinSi
			FinSi
		FinSi
	FinSi
	
FinAlgoritmo

# Ejercicio 9, PDF 2

Algoritmo Ejercicio9Parte2
	// Crear un programa que permita ingresar un nombre y una cantidad
	//numerica para que asi despues del programa escriba este nombre
	//tantas veces como su cantidad ingresada.
	Escribir "Introduce un nombre."
	Leer nombre
	
	Escribir "Introduce una cantidad de veces a repetir el nombre."
	Leer repeticiones
	
	Para contador<-1 Hasta repeticiones Hacer
		Escribir nombre
	Fin Para
FinAlgoritmo

# Ejercicio 10, PDF 2

Algoritmo Ejercicio10Parte2
	//Mostrar los multiplos de 3 desde 1 hasta el numero n, ingresado por teclado.
	Escribir "Introduce un numero."
	Leer n
	
	Para contador<-1 Hasta n Hacer
		multipito=contador*3
		Escribir multipito
	Fin Para
	
FinAlgoritmo

# Ejercicio 11, PDF 2

Algoritmo Ejercicio11Parte2
	//imprimir de forma descendente los 30 primeros numeros naturales menores de 30
	Para contador<-30 Hasta 0 Con Paso -1 Hacer
		Escribir contador
	Fin Para
FinAlgoritmo

# Ejercicio 12, PDF 2

Algoritmo Ejercicio12Parte2
	//algoritmo que visualice los numeros que son multiplos
	//de 2 o de 3 que hay entre 1 y 100
	Para contador<-1 Hasta 100 Hacer
		Si contador%2=0 Entonces
			Escribir contador
		SiNo
			Si contador %3=0 Entonces
				Escribir contador
			FinSi
		FinSi
	Fin Para
FinAlgoritmo

# Ejercicio 13, PDF 2

Algoritmo Ejercicio13Parte2
	//Se pide representar el algoritmo que nos calcule la suma de los N primeros
	//numeros naturales. N se leera por teclado
	Escribir "Introduzca la cantidad de numeros naturales a sumar."
	Leer n
	
	a=0
	resultado=0
	
	Para contador<-1 Hasta n Hacer
		a=a+1
		resultado=resultado+a
	Fin Para
	Escribir resultado
FinAlgoritmo
